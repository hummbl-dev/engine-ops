{
    "name": "hummbl-engine-ops",
    "version": "0.3.0",
    "description": "Enterprise-grade optimization engine with REST API, WebSocket, advanced algorithms, multi-language SDKs, Prometheus metrics, authentication, and production hardening",
    "main": "dist/public/api.js",
    "types": "dist/public/api.d.ts",
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "keywords": [
        "optimization",
        "scheduling",
        "resource-allocation",
        "bin-packing",
        "performance",
        "rest-api",
        "kubernetes",
        "caching",
        "metrics",
        "typescript"
    ],
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "start": "node dist/public/server.js",
        "dev": "nodemon --watch public --watch core --watch schemas --ext ts --exec \"npm run build && npm start\"",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write .",
        "docs": "typedoc --out docs/api",
        "migrate": "npm run build && node dist/infra/scripts/migrate.js",
        "migrate:status": "npm run build && node dist/infra/scripts/migrate.js status",
        "migrate:rollback": "npm run build && node dist/infra/scripts/migrate.js down"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hummbl-dev/engine-ops.git"
    },
    "author": "HUMMBL, LLC <hummbldev@gmail.com>",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/hummbl-dev/engine-ops/issues"
    },
    "homepage": "https://github.com/hummbl-dev/engine-ops#readme",
    "devDependencies": {
        "@jest/globals": "^30.2.0",
        "@types/bcrypt": "^6.0.0",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/jest": "^30.0.0",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.10.1",
        "@types/serve-static": "^2.2.0",
        "@types/superagent": "^8.1.9",
        "@types/supertest": "^6.0.3",
        "@types/swagger-ui-express": "^4.1.8",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "artillery": "^2.0.21",
        "eslint": "^8.0.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.11",
        "prettier": "^3.0.0",
        "supertest": "^7.1.4",
        "ts-jest": "^29.4.5",
        "ts-node": "^10.9.1",
        "typedoc": "^0.28.14",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@aws-sdk/client-secrets-manager": "^3.936.0",
        "@kubernetes/client-node": "^1.4.0",
        "@types/js-yaml": "^4.0.9",
        "@types/socket.io": "^3.0.1",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0",
        "js-yaml": "^4.1.1",
        "node-vault": "^0.10.9",
        "prom-client": "^15.1.3",
        "socket.io": "^4.8.1",
        "swagger-ui-express": "^5.0.1",
        "zod": "^4.1.12"
    }
}
